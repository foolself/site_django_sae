{% extends 'base.html' %}
{% block content %}
{% for article in article_list %}
<div class="panel panel-default">
  <div class="panel-body">
    <h1><a href="{% url 'blog.views.article_detail' pk=article.pk %}">{{ article.title }}</a></h1>
    <p>{{ article.author }} {{ article.published_date }}</p>
    <p>{{ article.text | striptags | truncatechars:100}}</p>
  </div>
</div>

{% endfor %}
{% endblock content %}