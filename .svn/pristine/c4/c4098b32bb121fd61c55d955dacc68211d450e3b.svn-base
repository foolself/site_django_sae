    <link href="/static/prism.css" rel="stylesheet" />
  </head>

  <body role="document">
  <script src="/static/prism.js" ></script>
    <!-- Fixed navbar -->
    <nav class="navbar navbar-inverse navbar-static-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
            <img class="imagehead" src="\static\image\head.jpg">
          <a class="navbar-brand" href="#">Foolself Blog</a>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
              <li class="active" ><a href="{% url 'home'%}"><span class="glyphicon glyphicon-home">Home</span></a></li>
              <li><a href="{% url 'about'%}"><span class="glyphicon glyphicon-info-sign">About</span></a></li>
              <li><a href="{% url 'message'%}"><span class="glyphicon glyphicon-home">Message</span></a></li>
              {% if user.is_authenticated %}
                <li><a href="{% url 'article_draft_list'%}"><span class="glyphicon glyphicon-file">Draft</span></a></li>
              <li><a href="{% url 'article_new'%}"><span class="glyphicon glyphicon-plus">Add</span></a></li>
              {% endif %}
          </ul>
          <ul class="nav navbar-nav navbar-right">
              {% if user.is_authenticated %}
              <li><a href="{% url 'logout'%}"><span class="glyphicon glyphicon-log-out">Logout</span></a></li>
              {% else %}
              <li><a href="{% url 'login'%}"><span class="glyphicon glyphicon-log-in">Login</span></a></li>
              {% endif %}
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>
  <div class="container theme-showcase" role="main">


        <div class="row">
            <div class="col-md-9">
			    {% block content %}
			    {% endblock %}
		    </div>
            <div class="col-md-3">
                <div class="panel panel-success">
                    <div class="panel-heading">
                    <h3 class="panel-title">Category</h3>
                    </div>
                    {% for category in category_list %}
                    <div class="panel-body">
                        <a href="#">{{ category }}&nbsp&nbsp<span class="badge">14</span></a>
                    </div>
                    {% endfor %}
                </div>
<br>
<h2>Tags</h2>
<hr class="featurette-divider">
{% for key,value in tag_dict.items %}
        <button type="button" class="{{ value }}">{{ key }}</button>
{% endfor %}

            </div>
        </div>

        <br><br>

<hr class="featurette-divider">
<footer>
<p><p class="pull-left">&copy; 2014 - 2016 Foolself. All Rights Reserved. </p><p class="pull-right"> Email: wi1191122731@gmail.com</p></p>
</footer>
</div> <!-- /container -->
